set_verbose(1);

load_file();

set_base_size(0.02);
set_default_growth_rate("moderate");  //- verySlow, slow, moderate, fast, custom
set_boundary_growth_rate("moderate"); //- verySlow, slow, moderate, fast, custom

set_surf_size_value_type("relativeToBase"); //- relativeToBase, absolute
set_surf_size_method("minAndTarget");      //- minOnly, minAndTarget
set_surf_min_size(0.02);
set_surf_target_size(1.0);


set_curvature_type("continum");  //- continum, custom, disableExternalCapture
set_curvature_span_angle(0.5);

save_to("meshReference");