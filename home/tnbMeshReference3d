setVerbose(1);

loadFile();

setBaseSize(0.02);
setDefaultGrowthRate("moderate");  //- verySlow, slow, moderate, fast, custom
setBoundaryGrowthRate("moderate"); //- verySlow, slow, moderate, fast, custom

setSurfSizeValueType("relativeToBase"); //- relativeToBase, absolute
setSurfSizeMethod("minAndTarget");      //- minOnly, minAndTarget
setSurfMinSize(0.02);
setSurfTargetSize(1.0);


setCurvatureType("continum");  //- continum, custom, disableExternalCapture
setCurvatureSpanAngle(0.5);

save_to("meshReference");